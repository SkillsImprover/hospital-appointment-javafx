<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<GridPane alignment="center" hgap="40.0" prefHeight="100.0" prefWidth="300.0" vgap="40"
          xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="com.stormnet.dentapp.client.controllers.registration.InvitationController">
    <Button onAction="#clientBtnPressed" mnemonicParsing="false" text="Client" GridPane.columnIndex="0"
            GridPane.rowIndex="0"/>
    <Button onAction="#doctorBtnPressed" mnemonicParsing="false" text="Doctor" GridPane.columnIndex="1"
            GridPane.rowIndex="0"/>
    <Button onAction="#adminBtnPressed" mnemonicParsing="false" text="Admin" GridPane.columnIndex="2"
            GridPane.rowIndex="0"/>

</GridPane>
